{
  "dependencies": {
    "microservice-config": {
      "url": "github.com:vicjicaman/microservice-config.git",
      "version": "1.70.3-master"
    },
    "microservice-resources-config": {
      "url": "github.com:vicjicaman/microservice-resources-config.git",
      "version": "1.70.6-master"
    }
  },
  "config": [
    {
      "name": "WEB_DEPLOYMENT_NAME",
      "value": "resources-web-deployment"
    },
    {
      "name": "CACHE_VOLUME_NAME",
      "value": "blog-repoflow-resources-cache-volume"
    },
    {
      "name": "CACHE_CLAIM_NAME",
      "value": "resources-cache-volume-claim"
    },
    {
      "name": "CACHE_STATEFUL_NAME",
      "value": "resources-cache-stateful"
    },
    {
      "name": "DATA_VOLUME_NAME",
      "value": "blog-repoflow-resources-data-volume"
    },
    {
      "name": "DATA_CLAIM_NAME",
      "value": "resources-data-volume-claim"
    },
    {
      "name": "DATA_STATEFUL_NAME",
      "value": "resources-data-stateful"
    },
    {
      "name": "QUEUE_VOLUME_NAME",
      "value": "blog-repoflow-resources-queue-volume"
    },
    {
      "name": "QUEUE_CLAIM_NAME",
      "value": "resources-queue-volume-claim"
    },
    {
      "name": "QUEUE_STATEFUL_NAME",
      "value": "resources-queue-stateful"
    },
    {
      "name": "RESOURCES_SERVICE_NAME",
      "value": "resources-external-service"
    },
    {
      "name": "RESOURCES_SERVICE_PORT",
      "value": 80
    },
    {
      "name": "RESOURCES_EXTERNAL_DOMAIN",
      "value": "s3.amazonaws.com"
    },
    {
      "name": "RESOURCES_EXTERNAL_PATH",
      "value": "/resources.repoflow.com${WEB_BASE_ROUTE@microservice-resources-config}"
    }
  ]
}
